#. Console/s
msgid "s_doppler"
msgstr "<value>	If enabled, Doppler effect will be used for sounds"

#. Console/s
msgid "s_globalfocus"
msgstr "<0/1> Mutes the game when alt-tabbed. 0 = Disabled, 1 = Enabled"

#. Console/s
msgid "s_khz"
msgstr "<value>	The sampling rate of the sounds in kilohertz."

#. Console/s
msgid "s_mixahead"
msgstr "<value>	The specified time waited before mixing sound samples."

#. Console/s
msgid "s_module"
msgstr "<0-2> Specifies the default sound module. 0 = No Sound, 1 = Qfusion, 2 = OpenAL"

#. Console/s
msgid "s_module_fallback"
msgstr "<0-2> Specifies a fallback sound module incase s_module cannot be loaded."

#. Console/s
msgid "s_musicvolume"
msgstr "<0-1> Global music volume."
	
#. Console/s
msgid "s_openAL_device"
msgstr "<value>	Specifies the OpenAL device. Example: s_openAL_device 'Rapture3D'"
	
#. Console/s
msgid "s_pseudoAcoustics"
msgstr "<0/1> High quality spatialisation for the Qfusion Sound Module."

#. Console/s
msgid "s_separationDelay"
msgstr "Please help contribute a description of this command by visiting the Warfork github repository."

#. Console/s
msgid "s_stereo2mono"
msgstr "<0/1> Enables mono sound."

#. Console/s
msgid "s_swapstereo"
msgstr "<0/1> Swap the left and right sound channel."

#. Console/s
msgid "s_volume"
msgstr "<0-1> Global volume of the game."

#. Console/s
msgid "s_wavonly"
msgstr "<0/1> If enabled, only .wav sound files will be played."

#. Console/s
msgid "s_devices"
msgstr "List of available OpenAL sound devices."

#. Console/s
msgid "s_restart"
msgstr "Restarts the sound engine."
